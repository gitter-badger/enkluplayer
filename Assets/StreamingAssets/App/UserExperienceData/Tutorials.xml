  <!-- Concept: OnExplore -->
  <UserExperienceData>
    <Id>ConceptOnExplore</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>ConceptExplore</Event>
    </Trigger>

    <!-- Tutorial -->
    <TutorialActions>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Unhighlight</Type>
      </ActionData>
      <ActionData>
        <Text>Review keywords before moving to the next [color=VColor.Highlight]Concept[/color].</Text>
      </ActionData>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>Concept</Id>
        </Content>
        <Text>ShowNavigatorWidget</Text>
      </ActionData>
    </TutorialActions>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>Concept</Id>
        </Content>
        <Text>ShowNavigatorWidget</Text>
      </ActionData>
    </Actions>

  </UserExperienceData>

  <!-- Trophy: OnPlaced -->
  <UserExperienceData>
    <Id>TrophyPlaced</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>Message</Event>
      <Id>TrophyPlaced</Id>
    </Trigger>

    <!-- Tutorial -->
    <Actions>

      
      <ActionData>
        <Delay>1</Delay>
        <Text>Congratulations!</Text>
      </ActionData>
      <ActionData>
        <Text>You have completed the [color=VColor.Highlight]Spire[/color] demonstration!</Text>
      </ActionData>

      <ActionData>
        <Text>On behalf of Create AR</Text>
      </ActionData>
      <ActionData>
        <Text>I would like to give a special thanks to [color=VColor.Highlight]Pete Worrell[/color], [color=VColor.Highlight]Denise Burke[/color], and the rest of the Bigelow LLC team!</Text>
      </ActionData>
      <ActionData>
        <Type>Music</Type>
      </ActionData>
      <ActionData>
        <Text>But before you go...</Text>
      </ActionData>
      
      <ActionData>
        <Type>Prompt</Type>
        <Video>
          <ContentId>blaw</ContentId>
        </Video>
      </ActionData>
      
      <ActionData>
        <Type>Prompt</Type>
        <MultipleChoice>
          <Title></Title>
          <Prompt>How would you describe augmented reality to a friend or colleague?</Prompt>
          <Options>
            <ButtonData>
              <Caption>It's the future!</Caption>
              <ImageId>Up</ImageId>
            </ButtonData>
            <ButtonData>
              <Caption>It's already here!</Caption>
              <ImageId>Yes</ImageId>
            </ButtonData>
            <ButtonData>
              <Caption>It's not quite ready.</Caption>
              <ImageId>Down</ImageId>
            </ButtonData>
          </Options>
        </MultipleChoice>
      </ActionData>

      <ActionData>
        <Text>Thanks for the feedback!</Text>
      </ActionData>
      <ActionData>
        <Text>Please let the attendent know that you are finished.</Text>
      </ActionData>
    </Actions>
  </UserExperienceData>

  <!-- ShowPlacementPrompt -->
  <UserExperienceData>
    <Id>ShowPlacementPrompt</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>Message</Event>
      <Id>ShowPlacementPrompt</Id>
    </Trigger>
    <!-- Show the placement window -->
    <Actions>
      <ActionData>
        <Type>Show</Type>
        <Content>
          <Id>Floor</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Show</Type>
        <Content>
          <Id>Model3</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Prompt</Type>
        <Movement>
          <Title>Placement</Title>
          <Prompt>Reposition the Turbofan with your head to find a [color=VColor.Highlight]clear area[/color] nearby.</Prompt>
          <MustMoveBeforeConfirm>false</MustMoveBeforeConfirm>
          <ContentId>Floor</ContentId>
          <CheckCollided>true</CheckCollided>
          <CancelEnabled>false</CancelEnabled>
        </Movement>
      </ActionData>
      <ActionData>
        <Type>Hide</Type>
        <Content>
          <Id>Model3</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Event</Type>
        <Id>ShowCalibratePrompt</Id>
      </ActionData>
    </Actions>
  </UserExperienceData>

