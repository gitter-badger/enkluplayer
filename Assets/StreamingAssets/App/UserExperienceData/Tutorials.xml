<ArrayOfUserExperienceData>

  <!-- Startup: OnCalibrate -->
  <UserExperienceData>
    <Id>StartupOnCalibrate</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>Message</Event>
      <Id>BeginCalibrate</Id>
    </Trigger>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>Music</Type>
        <Id>Tycho - Source</Id>
      </ActionData>
      
      <ActionData>
        <Delay>0.5</Delay>
        <Type>ShowWidget</Type>
        <Content>
          <Id>SAI</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Prompt</Type>
        <MultipleChoice>
          <Title>Welcome</Title>
          <Prompt>Move the cursor with your head to activate the Ok button to the right.</Prompt>
          <Options>
            <ButtonData>
              <Caption>Ok</Caption>
              <ImageId>Yes</ImageId>
              <Highlight>true</Highlight>
              <Priority>5</Priority>
              <Actions>
                <ActionData>
                  <Type>Event</Type>
                  <Id>ShowCalibratePrompt</Id>
                </ActionData>
              </Actions>
            </ButtonData>
            <ButtonData>
              <Caption>Login</Caption>
              <ImageId>Key</ImageId>
              <EditorOnly>true</EditorOnly>
              <Actions>
                <ActionData>
                  <Type>State</Type>
                  <Content>
                    <Id>Login</Id>
                  </Content>
                </ActionData>
              </Actions>
            </ButtonData>
            <ButtonData>
              <Caption>Explore</Caption>
              <ImageId>Map</ImageId>
              <EditorOnly>true</EditorOnly>
              <Actions>
                <ActionData>
                  <Type>State</Type>
                  <Content>
                    <Id>Explore</Id>
                  </Content>
                </ActionData>
              </Actions>
            </ButtonData>
            <ButtonData>
              <Caption>Options</Caption>
              <ImageId>Setup</ImageId>
              <EditorOnly>true</EditorOnly>
              <Actions>
                <ActionData>
                  <Type>State</Type>
                  <Content>
                    <Id>Options</Id>
                  </Content>
                </ActionData>
              </Actions>
            </ButtonData>
          </Options>
        </MultipleChoice>
      </ActionData>
      
    </Actions>
  </UserExperienceData>

  <!-- ShowCalibratePrompt -->
  <UserExperienceData>
    <Id>ShowCalibratePrompt</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>Message</Event>
      <Id>ShowCalibratePrompt</Id>
    </Trigger>

    <!-- Show the calibration window -->
    <Actions>
      <ActionData>
        <Type>ShowWidget</Type>
        <Content>
          <Id>Corners Widget</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Prompt</Type>
        <Detail>
          <Button>
            <Caption> </Caption>
            <ImageId>Speak</ImageId>
            <HiddenVoiceActivationString>Continue</HiddenVoiceActivationString>
            <ButtonAssetId>StandardNoHighlight</ButtonAssetId>
          </Button>
          <ImageId>Adjustment</ImageId>
          <Details>Make sure the [color=VColor.Highlight2]headband[/color] sits snugly on your forehead. Rest the [color=VColor.Highlight3]nose pad[/color] lightly on your nose. You may also slide the headset forward and backward. Turn the [color=VColor.Highlight4]adjustment wheel[/color] to tighten the handle for a snug fit.\n\nWhen you're comfortable and can see all [color=VColor.Tertiary]four corners[/color] in the frame, say [Color=VColor.Highlight]Continue[/color].</Details>
        </Detail>
      </ActionData>
      <ActionData>
        <Type>Hide</Type>
        <Content>
          <Id>Corners Widget</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>StartupController</Id>
        </Content>
        <Text>CalibrationComplete</Text>
      </ActionData>
    </Actions>
  </UserExperienceData>

  <!-- Module on Review Complete -->
  <UserExperienceData>
    <Id>ModuleOnReviewComplete</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>ModuleReviewComplete</Event>
    </Trigger>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Modules</Id>
        </Content>
      </ActionData>
    </Actions>

  </UserExperienceData>

  <!-- OnAuthenticated -->
  <UserExperienceData>
    <Id>OnAuthenticated</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>Message</Event>
      <Id>Authenticated</Id>
    </Trigger>

    <!-- Tutorial -->
    <Actionss>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Certification</Id>
        </Content>
      </ActionData>
    </Actionss>
    <Actions>
      <ActionData>
        <Type>Music</Type>
        <Id>Poppy Ackroyd - Time</Id>
      </ActionData>
      <ActionData>
        <Text>Hello [color=VColor.Highlight][UserName][/color]!</Text>
      </ActionData>
      <ActionData>
        <Text>Welcome to [color=VColor.Highlight]Spire[/color], The Pinnacle of Training</Text>
      </ActionData>
      <ActionData>
        <Text>This demonstration includes [color=VColor.Highlight]Instruction[/color] and [color=VColor.Highlight]3D Reference[/color] in augmented reality</Text>
      </ActionData>
      <ActionData>
        <Delay>0.25</Delay>
        <Type>Animate</Type>
        <Id>Happy</Id>
      </ActionData>
      <ActionData>
        <Text>My name is [color=VColor.Positive]SAI[/color], and I'll be your guide</Text>
      </ActionData>
      <ActionData>
        <Text>Before we get started, I have a question</Text>
      </ActionData>
      <ActionData>
        <Type>Prompt</Type>
        <MultipleChoice>
          <Title></Title>
          <Prompt>Would you like me to adjust the speed of my [color=VColor.Highlight]Speech[/color]?</Prompt>
          <Options>
            <ButtonData>
              <Caption>Faster, please.</Caption>
              <ImageId>Up</ImageId>
              <Actions>
                <ActionData>
                  <Type>Speech</Type>
                  <Id>SpeakQuickly</Id>
                </ActionData>
              </Actions>
            </ButtonData>
            <ButtonData>
              <Caption>It's great!</Caption>
              <ImageId>Yes</ImageId>
            </ButtonData>
            <ButtonData>
              <Caption>Slower would be better.</Caption>
              <ImageId>Down</ImageId>
              <Actions>
                <ActionData>
                  <Type>Speech</Type>
                  <Id>SpeakSlowly</Id>
                </ActionData>
              </Actions>
            </ButtonData>
          </Options>
        </MultipleChoice>
      </ActionData>

      <ActionData>
        <Text>Thanks for the feedback!</Text>
      </ActionData>
      <ActionData>
        <Text>You can personalize me to fit your learning style</Text>
      </ActionData>
      <ActionData>
        <Text>Let's try out a new voice</Text>
      </ActionData>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Options</Id>
        </Content>
      </ActionData>

      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>TextToSpeechVoice Next Button</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Wait</Type>
      </ActionData>

      <ActionData>
        <Type>Unhighlight</Type>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Delay>3.0</Delay>
        <Text>When you're ready, let's navigate to the [color=VColor.Highlight]Home Menu[/color] by activating the Home button on the left.</Text>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Lock</Type>
        <Content>
          <Id>Reset Progress Button</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>Back Button</Id>
        </Content>
        <Wait>true</Wait>
      </ActionData>
    </Actions>

    <Actionss>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Home</Id>
        </Content>
      </ActionData>
    </Actionss>

  </UserExperienceData>

  <!-- Home: OnEnter -->
  <UserExperienceData>
    <Id>HomeOnEnter</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>StateEnter</Event>
      <Id>Home</Id>
    </Trigger>

    <Requirements>
      <NotModules>
        <string>Components</string>
      </NotModules>
    </Requirements>
    
    <!-- Tutorial -->
    <Actions>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Music</Type>
        <Id>Saib. - Spike Spiegel</Id>
      </ActionData>
      <ActionData>
        <Text>Let's get started</Text>
      </ActionData>
      <ActionData>
        <Text>This is a [color=VColor.Highlight]3D Map[/color] of an innovative new form of Turbine Engine</Text>
      </ActionData>
      
      <ActionData>
        <Delay>0.5</Delay>
        <Text>Move around carefully to get a closer look</Text>
      </ActionData>
      <ActionData>
        <Type>Expand</Type>
        <Content>
          <Id>Jet Engine</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      
      <ActionData>
        <Delay>5</Delay>
        <Text>When you're ready to move on</Text>
      </ActionData>
      <ActionData>
        <Text>Follow me with your gaze and select the [color=VColor.Highlight]Turbine[/color] module</Text>
      </ActionData>
      <ActionData>
        <Type>Lock</Type>
        <Content>
          <Id>Jet Engine Toggle</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Unhighlight</Type>
        <Content>
          <Id>Jet Engine Toggle</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>Turbine Toggle</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Wait</Type>
      </ActionData>
      <ActionData>
        <Type>Unhighlight</Type>
        <Content>
          <Id>Turbine Toggle</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Show</Type>
        <Content>
          <Id>Foreground</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Text>Inside a [color=VColor.Highlight]Module[/color]</Text>
      </ActionData>
      <ActionData>
        <Text>You can access detailed information and test your comprehension</Text>
      </ActionData>
      <ActionData>
        <Text>Select [color=VColor.Highlight]GEARED TURBOFAN[/color] to begin!</Text>
      </ActionData>
      <ActionData>
        <Type>Hide</Type>
        <Content>
          <Id>Foreground</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>Geared Turbofan</Id>
        </Content>
      </ActionData>
    </Actions>

  </UserExperienceData>

  <!-- Modules: OnEnter 1st -->
  <UserExperienceData>
    <Id>ModulesOnEnter1st</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>StateEnter</Event>
      <Id>Modules</Id>
    </Trigger>
    
    <Requirements>
      <NotModules>
        <string>Introduction</string>
      </NotModules>
    </Requirements>

    <!-- Tutorial -->
    <TutorialActions>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Text>In the [color=VColor.Highlight]Modules Menu[/color]</Text>
      </ActionData>
      <ActionData>
        <Text>You can access newly unlocked Modules</Text>
      </ActionData>
      <ActionData>
        <Text>Let's begin with an introduction to the [color=VColor.Highlight]Geared Turbofan[/color]!</Text>
      </ActionData>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>ModulesUI</Id>
        </Content>
        <Text>HighlightNextModule</Text>
      </ActionData>
    </TutorialActions>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>ModulesUI</Id>
        </Content>
        <Text>HighlightNextModule</Text>
      </ActionData>
    </Actions>

  </UserExperienceData>

  <!-- Modules: OnEnter 2nd -->
  <UserExperienceData>
    <Id>ModulesOnEnter2nd</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>StateEnter</Event>
      <Id>Modules</Id>
    </Trigger>

    <Requirements>
      <Modules>
        <string>Introduction</string>
      </Modules>
      <NotModules>
        <string>Components</string>
      </NotModules>
    </Requirements>

    <!-- Tutorial -->
    <TutorialActions>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Text>Let's continue by learning about the [color=VColor.Highlight]Components[/color] of the HV Battery!</Text>
      </ActionData>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>ModulesUI</Id>
        </Content>
        <Text>HighlightNextModule</Text>
      </ActionData>
    </TutorialActions>

  </UserExperienceData>

  <!-- Modules: OnEnter -->
  <UserExperienceData>
    <Id>ModulesOnEnter</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>StateEnter</Event>
      <Id>Modules</Id>
    </Trigger>

    <Requirements>
      <Modules>
        <string>Components</string>
      </Modules>
    </Requirements>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>Method</Type>
        <Content>
          <Id>ModulesUI</Id>
