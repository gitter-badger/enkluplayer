  <!-- Module: OnReviewComplete -->
  <UserExperienceData>
    <Id>ModuleOnReviewComplete</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>ModuleReviewComplete</Event>
    </Trigger>

    <!-- Tutorial -->
    <TutorialActions>
      <ActionData>
        <Text>Before we continue, I have a question</Text>
      </ActionData>
      <ActionData>
        <Type>Prompt</Type>
        <MultipleChoice>
          <Title></Title>
          <Prompt>Would you like me to adjust the speed of my [color=VColor.Highlight]Speech[/color]?</Prompt>
          <Options>
            <ButtonData>
              <Caption>Faster, please.</Caption>
              <ImageId>Up</ImageId>
              <Actions>
                <ActionData>
                  <Type>Speech</Type>
                  <Id>SpeakQuickly</Id>
                </ActionData>
              </Actions>
            </ButtonData>
            <ButtonData>
              <Caption>It's great!</Caption>
              <ImageId>Yes</ImageId>
            </ButtonData>
            <ButtonData>
              <Caption>Slower would be better.</Caption>
              <ImageId>Down</ImageId>
              <Actions>
                <ActionData>
                  <Type>Speech</Type>
                  <Id>SpeakSlowly</Id>
                </ActionData>
              </Actions>
            </ButtonData>
          </Options>
        </MultipleChoice>
      </ActionData>
      
      <ActionData>
        <Text>Thanks for the feedback!</Text>
      </ActionData>
      <ActionData>
        <Text>You can personalize me to fit your learning style</Text>
      </ActionData>
      <ActionData>
        <Text>Let's try out a new voice</Text>
      </ActionData>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Options</Id>
        </Content>
      </ActionData>
      
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>TextToSpeechVoice Next Button</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Wait</Type>
      </ActionData>

      <ActionData>
        <Type>Unhighlight</Type>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Delay>3.0</Delay>
        <Text>When you're ready, let's return to the [color=VColor.Highlight]Home Menu[/color]</Text>
      </ActionData>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Type>Lock</Type>
        <Content>
          <Id>Reset Progress Button</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Type>Highlight</Type>
        <Content>
          <Id>Back Button</Id>
        </Content>
        <Wait>true</Wait>
      </ActionData>
    </TutorialActions>

    <!-- Actions -->
    <Actions>
      <ActionData>
        <Type>State</Type>
        <Content>
          <Id>Modules</Id>
        </Content>
      </ActionData>
    </Actions>

  </UserExperienceData>

  <!-- Certification: OnComplete -->
  <UserExperienceData>
    <Id>CertificationOnComplete</Id>
    <Trigger>
      <Type>OnEvent</Type>
      <Event>CertificationComplete</Event>
    </Trigger>

    <!-- Tutorial -->
    <TutorialActions>
      <ActionData>
        <Type>Rails</Type>
      </ActionData>
      <ActionData>
        <Text>You have unlocked the [color=VColor.Highlight]Map[/color]!</Text>
      </ActionData>
      <ActionData>
        <Text>In the [color=VColor.Highlight]Map[/color], you can review parts from completed Modules.</Text>
      </ActionData>
      <ActionData>
        <Delay>0.5</Delay>
        <Type>State</Type>
        <Content>
          <Id>Explore</Id>
        </Content>
      </ActionData>
      <ActionData>
        <Text>Carefully move around to get a closer look at the [color=VColor.Highlight2]3D Structure[/color].</Text>
      </ActionData>
      <ActionData>
        <Delay>5</Delay>
        <Text>Expand parts to explore their internal components in greater detail.</Text>
      </ActionData>
    </TutorialActions>
  </UserExperienceData>

