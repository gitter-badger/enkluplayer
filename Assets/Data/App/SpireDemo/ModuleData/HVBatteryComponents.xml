<ArrayOfModuleData>

  <!-- 2. Components -->
  <ModuleData>
    <Id>Components</Id>
    <CourseId>HV Battery</CourseId>
    <Anchor>
      <Type>Locator</Type>
      <LocatorId>Section 2</LocatorId>
    </Anchor>
    <LinkRootLocatorId>Section 2 Root</LinkRootLocatorId>
    
    <Requirements>
      <Modules>
        <string>Introduction</string>
      </Modules>
    </Requirements>
    
    <Concepts>

      <!-- Concept 1: Battery Configurations -->
      <ConceptData>
        <Id>Battery Configurations</Id>

        <Keywords>
          <KeywordData>
            <Keyword>Battery Configurations</Keyword>
            <VirtualColor>Highlight2</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>50 kWh</Keyword>
            <VirtualColor>Highlight2</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>75 kWh</Keyword>
            <VirtualColor>Highlight3</VirtualColor>
          </KeywordData>
        </Keywords>

        <Actions>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>BatteryConfigurationsAnimation</Id>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Foreground</ContentId>
                <LocatorId>Billboard</LocatorId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
            <Wait>false</Wait>
          </ActionData>

          <ActionData>
            <Text>Model 3 is currently being offered with one of two Battery Configurations</Text>
          </ActionData>
          <ActionData>
            <Text>A 50 kWh version, and a 75 kWh version</Text>
          </ActionData>
          <ActionData>
            <Text>Both versions are composed of the same basic components.</Text>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>BatteryConfigurationsAnimation</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

        </Actions>

      </ConceptData>

      <!-- Concept 2: Modules -->
      <ConceptData>
        <Id>Modules</Id>

        <Keywords>
          <KeywordData>
            <Keyword>Modules</Keyword>
            <VirtualColor>Highlight</VirtualColor>
          </KeywordData>

          <KeywordData>
            <Keyword>Battery Module 1 Covered</Keyword>
            <VirtualColor>Highlight2</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>Battery Module 2 Covered</Keyword>
            <VirtualColor>Highlight3</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>Battery Module 3 Covered</Keyword>
            <VirtualColor>Highlight4</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>Battery Module 4 Covered</Keyword>
            <VirtualColor>Highlight5</VirtualColor>
          </KeywordData>
        </Keywords>

        <Actions>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Text>Each Model 3 battery has four Modules that hold the bricks of battery cells.</Text>
            <Wait>false</Wait>
          </ActionData>
          
          <!-- Show Components -->

          <ActionData>
            <Delay>1</Delay>
            <Type>Show</Type>
            <Content>
              <Id>Battery Platter</Id>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Foreground</ContentId>
                <LocatorId>Highlight</LocatorId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Battery Module 1 Covered</Id>
              <PreserveColor>true</PreserveColor>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Battery Platter</ContentId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Battery Module 2 Covered</Id>
              <PreserveColor>true</PreserveColor>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Battery Platter</ContentId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Battery Module 3 Covered</Id>
              <PreserveColor>true</PreserveColor>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Battery Platter</ContentId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Battery Module 4 Covered</Id>
              <PreserveColor>true</PreserveColor>
              <Anchor>
                <Type>Locator</Type>
                <ContentId>Battery Platter</ContentId>
                <Scale>Identity</Scale>
                <Rotation>Identity</Rotation>
              </Anchor>
            </Content>
          </ActionData>

          <!-- Magnet Battery Platter to Model 3 -->
          
          <ActionData>
            <Delay>2</Delay>
            <Type>Show</Type>
            <Content>
              <Id>Model3</Id>
            </Content>
            <VirtualColor>Disabled</VirtualColor>
          </ActionData>

          <ActionData>
            <Type>Magnet</Type>
            <Delay>1</Delay>
            <Content>
              <Id>Battery Platter</Id>
              <Anchor>
                <ContentId>Model3</ContentId>
                <LocatorId>High Voltage Battery</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <!-- Label Modules -->

          <ActionData>
            <Type>Label</Type>
            <Text>Module 1</Text>
            <Content>
              <Id>Battery Module 1 Covered</Id>
              <Anchor>
                <LocatorId>Locator</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Label</Type>
            <Text>Module 2</Text>
            <Content>
              <Id>Battery Module 2 Covered</Id>
              <Anchor>
                <LocatorId>Locator</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Label</Type>
            <Text>Module 3</Text>
            <Content>
              <Id>Battery Module 3 Covered</Id>
              <Anchor>
                <LocatorId>Locator</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Label</Type>
            <Text>Module 4</Text>
            <Content>
              <Id>Battery Module 4 Covered</Id>
              <Anchor>
                <LocatorId>Locator</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Delay>1</Delay>
          </ActionData>

        </Actions>

        <Assessments>
          <AssessmentData>
            <MultipleChoice>
              <Score>30</Score>
              <Prompt>How many Modules are in the Model 3 High Voltage Battery?</Prompt>
              <Correct>
                <int>2</int>
              </Correct>
              <Options>
                <ButtonData>
                  <Caption>2</Caption>
                </ButtonData>
                <ButtonData>
                  <Caption>3</Caption>
                </ButtonData>
                <ButtonData>
                  <Caption>4</Caption>
                </ButtonData>
              </Options>
            </MultipleChoice>
          </AssessmentData>
        </Assessments>

      </ConceptData>

      <!-- Concept 3: Current Collectors -->
      <ConceptData>
        <Id>Current Collectors</Id>

        <Keywords>
          <KeywordData>
            <Keyword>Current Collectors</Keyword>
            <VirtualColor>Highlight</VirtualColor>
          </KeywordData>
          <KeywordData>
            <Keyword>Collector Plate</Keyword>
            <VirtualColor>Highlight2</VirtualColor>
          </KeywordData>
        </Keywords>

        <Actions>

          <ActionData>
            <Type>Show</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <!-- Magnet Battery Platter to Model 3 -->

          <ActionData>
            <Type>Magnet</Type>
            <Delay>1</Delay>
            <Content>
              <Id>Battery Platter</Id>
              <Anchor>
                <ContentId>Foreground</ContentId>
                <LocatorId>Highlight</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Model3</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Battery Module 1 Covered</Id>
            </Content>
            <Duration>0.5</Duration>
            <Wait>false</Wait>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Battery Module 2 Covered</Id>
            </Content>
            <Duration>0.75</Duration>
            <Wait>false</Wait>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Battery Module 3 Covered</Id>
            </Content>
            <Duration>1.0</Duration>
            <Wait>false</Wait>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Battery Module 4 Covered</Id>
            </Content>
            <Duration>1.25</Duration>
          </ActionData>

          <!-- Show Current Collectors -->

          <ActionData>
            <Text>Beneath the cells of the modules are Current Collectors.</Text>
          </ActionData>

          <ActionData>
            <Text>To keep them in their correct location, the cells are glued to a Collector Plate.</Text>
            <Wait>false</Wait>
          </ActionData>

          <ActionData>
            <Type>ShowWidget</Type>
            <Content>
              <Id>Current Collectors</Id>
            </Content>
          </ActionData>

          <!-- Magnet Battery Platter to Model 3 -->

          <ActionData>
            <Delay>1</Delay>
            <Type>Show</Type>
            <Content>
              <Id>Model3</Id>
            </Content>
            <VirtualColor>Disabled</VirtualColor>
          </ActionData>

          <ActionData>
            <Type>Magnet</Type>
            <Delay>1</Delay>
            <Content>
              <Id>Battery Platter</Id>
              <Anchor>
                <ContentId>Model3</ContentId>
                <LocatorId>High Voltage Battery</LocatorId>
              </Anchor>
            </Content>
          </ActionData>

          <!-- Label Current Collector -->

          <ActionData>
            <Type>Label</Type>
            <Content>
              <Id>Current Collectors</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Type>Hide</Type>
            <Content>
              <Id>Foreground</Id>
            </Content>
          </ActionData>

          <ActionData>
            <Delay>1</Delay>
          </ActionData>
          
        </Actions>

        <ClearBeforeAssessment>true</ClearBeforeAssessment>

        <Assessments>
          <AssessmentData>
            <MultipleChoice>
              <Score>30</Score>
              <Prompt>Where are the Current Collectors located on Model 3?</Prompt>
              <Correct>
                <int>1</int>
              </Correct>
              <Options>
                <ButtonData>
                  <Caption>Top</Caption>
                </ButtonData>
                <ButtonData>
                  <Caption>Bottom</Caption>
                </ButtonData>
                <ButtonData>
                  <Caption>Bottom and top</Caption>
                </ButtonData>
              </Options>
            </MultipleChoice>
          </AssessmentData>
        </Assessments>

      </ConceptData>

